platform :ios, '10.0'

target 'CarloudyCodeChallenge' do
  use_frameworks!

  pod 'Mapbox-iOS-SDK'
  pod 'MapboxGeocoder.swift', :git => 'https://github.com/mapbox/MapboxGeocoder.swift.git'
  pod 'MapboxDirections.swift', :git => 'https://github.com/mapbox/MapboxDirections.swift.git'

  target 'CarloudyCodeChallengeTests' do
    inherit! :search_paths
    # Pods for testing
  end

end
